<?xml version="1.0" encoding="utf-8"?>

<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/TableLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
    <Spinner
        android:id="@+id/freq_func_spinner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

	<TableRow
        android:id="@+id/freq_tableRow1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >
		
		<TextView
			android:id="@+id/freq_divlabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="2"
            android:maxWidth="30dp"
            android:text="@string/divp2pString"
            android:textSize="12sp" 
			
			/>
		<EditText
            android:id="@+id/freq_div"
            android:layout_width="135dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/hint_String"
            android:inputType="numberDecimal" />

        <Spinner
            android:id="@+id/freq_divspinner"
            android:layout_width="100dp"
            android:layout_height="wrap_content" />
		
		
	</TableRow>
	
</TableLayout>
